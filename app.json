{
  "expo": {
    "name": "Wellness Quest",
    "slug": "wellness-quest-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "54.0.0",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F9F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wellness.quest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5F9F6"
      },
      "package": "com.wellness.quest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "wellness-quest",
    "platforms": ["ios", "android", "web"]
  }
}
